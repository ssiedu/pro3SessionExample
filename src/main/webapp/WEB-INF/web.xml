<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>VerifyUser</servlet-name>
  	<display-name>VerifyUser</display-name>
  	<description></description>
  	<servlet-class>com.ssi.sm.VerifyUser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletOne</servlet-name>
  	<display-name>ServletOne</display-name>
  	<description></description>
  	<servlet-class>com.ssi.sm.ServletOne</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletTwo</servlet-name>
  	<display-name>ServletTwo</display-name>
  	<description></description>
  	<servlet-class>com.ssi.sm.ServletTwo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>KillSession</servlet-name>
  	<display-name>KillSession</display-name>
  	<description></description>
  	<servlet-class>com.ssi.KillSession</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>VerifyUser</servlet-name>
  	<url-pattern>/VerifyUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletOne</servlet-name>
  	<url-pattern>/ServletOne</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletTwo</servlet-name>
  	<url-pattern>/ServletTwo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>KillSession</servlet-name>
  	<url-pattern>/KillSession</url-pattern>
  </servlet-mapping>
  
  <session-config>
  	<session-timeout>2</session-timeout>
  </session-config>
  
</web-app>
